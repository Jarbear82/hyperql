{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hyperql",
      "camelcase": "HyperQl",
      "title": "HyperQL",
      "scope": "source.hyperql",
      "file-types": [
        "hyperql",
        "hql"
      ],
      "injection-regex": "^hyperql$",
      "class-name": "TreeSitterHyperql"
    }
  ],
  "metadata": {
    "version": "0.14.0",
    "license": "MIT",
    "description": "HyperQL grammar for tree-sitter",
    "authors": [
      {
        "name": "jarbear82",
        "email": "jarbear82@gmail.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/jarbear82/hyperql",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": true
  }
}